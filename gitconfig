# --date = (relative|local|default|iso|rfc|short|raw) 

[user]
    name = USERNAME
    email = EMAIL
[github]
    user = LOGIN
[color]
    diff = auto
    status = auto
    branch = auto
    interactive = auto
[alias]
    st = status -sbu
    stat = status
    ci = commit
    br = branch
    bl = branch -av
    tl = tag -l
    ls = ls-files
    co = checkout
    df = diff
    dfs = diff --stat -r
    dfn = diff --name-status -r
    lg = log -p
    lga = log --graph --decorate --pretty=oneline --abbrev-commit
    i = remote -v
    who = shortlog -s --
    what = show -s --pretty='tformat:%h (%s, %ad)' --date=short
    aliases = !git config --get-regexp 'alias.*' | colrm 1 6 | sed 's/[ ]/ = /'
